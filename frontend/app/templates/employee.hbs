<h1>{{model.fullName}}</h1>

<div class="row">
  <div class="col-md-3">
    <div class="thumbnail">
      <img src={{model.image}}>
      <div class="caption">
        <h3>{{model.fullName}}</h3>
        <p>{{markdown-to-html model.bio}}</p>
      </div>
    </div>
  </div>
  <div class="col-md-9">
    <ul>
    {{#each model.skills as |skill| }}
      <li>{{ skill.technology }}</li>
    {{/each}}
    </ul>
  </div>
</div>

